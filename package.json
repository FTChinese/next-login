{
  "name": "@ftchinese/next-user",
  "version": "0.5.11",
  "description": "User setting system",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production PORT=4100 URL_PREFIX=/user node index.js",
    "server": "nodemon dist/index.js",
    "watch-ts": "tsc -w",
    "build-js": "rollup -c",
    "build-css:prod": "node-sass --output-style=compressed --source-map=build/production client/scss/main.scss build/production/main.css",
    "build-css:dist": "node-sass --output-style=expanded --source-map=build/dist client/scss/main.scss build/dist/main.css",
    "build-css": "npm run build-css:dist && npm run build-css:prod",
    "build": "build-js && build-css",
    "inline": "node ./util/inline.js",
    "build:html": "npm run build-js && npm run build-css:prod && npm run inline",
    "test": "jest",
    "postversion": "git push && git push --tags",
    "postinstall": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FTChinese/next-user.git"
  },
  "author": "weiguo.ni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FTChinese/next-user/issues"
  },
  "homepage": "https://github.com/FTChinese/next-user#readme",
  "dependencies": {
    "@hapi/joi": "^15.0.0",
    "csrf": "^3.0.6",
    "debug": "^4.1.1",
    "del": "^4.1.0",
    "gulp": "^4.0.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-csrf": "^3.0.7",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.10.1",
    "lodash": "^4.17.11",
    "luxon": "^1.9.0",
    "mobile-detect": "^1.4.3",
    "numeral": "^2.0.6",
    "nunjucks": "^3.1.7",
    "qrcode": "^1.3.3",
    "superagent": "^4.1.0",
    "toml": "^3.0.0",
    "ua-parser-js": "^0.7.19",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "bootstrap.native": "^2.0.25",
    "faker": "^4.1.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "inline-source": "^6.1.9",
    "jest": "^24.1.0",
    "koa-static": "^5.0.0",
    "node-sass": "^4.11.0",
    "rollup": "^1.10.1",
    "rollup-plugin-babel-minify": "^8.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "^1.9.3",
    "typescript": "^3.4.4",
    "watch": "^1.0.2"
  }
}
