{
  "name": "@ftchinese/next-user",
  "version": "0.0.1",
  "description": "User setting system",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "test": "ava",
    "postversion": "git push && git push --tags",
    "postinstall": "",
    "clean": "rm -rf dist/*",
    "profile": "NODE_ENV=production PORT=4100 node --prof index.js",
    "prod": "NODE_ENV=production PORT=4100 node index.js",
    "vegeta": "vegeta attack -targets=targets.txt -rate=100 -duration=60s > .tmp/results.bin",
    "report": "vegeta report -inputs=.tmp/results.bin -reporter=plot > .tmp/plot.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FTChinese/next-user.git"
  },
  "author": "weiguo.ni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FTChinese/next-user/issues"
  },
  "homepage": "https://github.com/FTChinese/next-user#readme",
  "dependencies": {
    "csrf": "^3.0.6",
    "debug": "^3.1.0",
    "ioredis": "^3.2.2",
    "joi": "^13.4.0",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-csrf": "^3.0.6",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.8.1",
    "lodash": "^4.17.10",
    "moment": "^2.21.2",
    "nunjucks": "^3.1.3",
    "superagent": "^3.8.3",
    "ua-parser-js": "^0.7.18",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "bootstrap": "^4.1.1",
    "bootstrap.native": "^2.0.23",
    "chance": "^1.0.16",
    "koa-static": "^5.0.0"
  }
}
